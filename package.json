{
  "name": "v8-bytecode-highlight",
  "displayName": "v8-bytecode-highlight",
  "description": "Syntax highlighting for v8 ignition bytecode disassembly.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "./images/icon.png",
  "publisher": "j4k0xb",
  "repository": {
    "type": "git",
    "url": "https://github.com/j4k0xb/v8-bytecode-highlight.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "ignition",
        "aliases": [
          "V8 Ignition Bytecode",
          "ignition"
        ],
        "extensions": [
          ".ign"
        ],
        "icon": {
          "light": "./images/v8-ignition-light.svg",
          "dark": "./images/v8-ignition-dark.svg"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ignition",
        "scopeName": "source.ignition",
        "embeddedLanguages": {
          "source.js": "javascript",
          "meta.embedded.block.javascript": "javascript"
        },
        "path": "./syntaxes/ignition.tmLanguage.json"
      }
    ]
  }
}
